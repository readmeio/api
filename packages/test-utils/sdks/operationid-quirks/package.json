{
  "name": "@api/operationid-quirks",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "openapi.json"
  ],
  "scripts": {
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@readme/api-core": "file:../../../../../core"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "tsup": {
    "cjsInterop": true,
    "clean": true,
    "dts": true,
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "minify": false,
    "shims": true,
    "sourcemap": true,
    "splitting": true
  }
}
