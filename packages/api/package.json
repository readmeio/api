{
  "name": "api",
  "version": "4.2.0",
  "description": "Generate an SDK from an OpenAPI definition",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/ @types/; npm run prebuild.packageConfig",
    "prebuild.packageConfig": "node -p \"'// This file is automatically updated by the build script.\\nexport const PACKAGE_NAME = \\'' + require('./package.json').name + '\\';\\nexport const PACKAGE_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/packageInfo.ts",
    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "nyc mocha \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/readmeio/api.git",
    "directory": "packages/api"
  },
  "homepage": "https://github.com/readmeio/api",
  "bugs": {
    "url": "https://github.com/readmeio/api/issues"
  },
  "author": "Jon Ursenbach <jon@readme.io>",
  "license": "MIT",
  "engines": {
    "node": "^14 || ^16"
  },
  "dependencies": {
    "@readme/oas-to-har": "^16.1.0",
    "@readme/openapi-parser": "^2.1.0",
    "datauri": "^4.1.0",
    "fetch-har": "^8.0.0",
    "find-cache-dir": "^3.3.1",
    "form-data-encoder": "^1.7.1",
    "formdata-node": "^4.3.2",
    "get-stream": "^6.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^10.1.5",
    "json-schema-traverse": "^1.0.0",
    "lodash.merge": "^4.6.2",
    "make-dir": "^3.1.0",
    "oas": "^18.2.1",
    "object-hash": "^3.0.0",
    "ts-morph": "^14.0.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^8.5.1",
    "@readme/oas-examples": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/find-cache-dir": "^3.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^9.1.0",
    "@types/object-hash": "^2.2.1",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "mocha": "^9.2.1",
    "mocha-chai-jest-snapshot": "^1.1.3",
    "mock-require": "^3.0.3",
    "nock": "^13.2.4",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "unique-temp-dir": "^1.0.0"
  },
  "prettier": "@readme/eslint-config/prettier",
  "nyc": {
    "exclude": [
      "dist/",
      "test/"
    ]
  }
}
