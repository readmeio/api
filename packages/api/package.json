{
  "name": "api",
  "version": "4.2.0",
  "description": "Generate an SDK from an OpenAPI definition",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/ @types/; npm run prebuild.packageConfig",
    "prebuild.packageConfig": "node -p \"'export const PACKAGE_NAME = \\'' + require('./package.json').name + '\\';\\nexport const PACKAGE_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/package.ts",
    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "tsc; jest --coverage",
    "test-watch": "tsc; jest --watch",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/readmeio/api.git",
    "directory": "packages/api"
  },
  "bugs": {
    "url": "https://github.com/readmeio/api/issues"
  },
  "author": "Jon Ursenbach <jon@readme.io>",
  "license": "MIT",
  "engines": {
    "node": "^14 || ^16"
  },
  "dependencies": {
    "@readme/oas-to-har": "^15.0.0",
    "@readme/openapi-parser": "^2.0.0",
    "datauri": "^4.1.0",
    "fetch-har": "^6.1.0",
    "find-cache-dir": "^3.3.1",
    "form-data-encoder": "^1.7.1",
    "formdata-node": "^4.3.2",
    "get-stream": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "make-dir": "^3.1.0",
    "oas": "^17.6.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^8.4.1",
    "@readme/oas-examples": "^4.4.0",
    "@types/find-cache-dir": "^3.2.1",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "memfs": "^3.4.1",
    "nock": "^13.2.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.0-beta"
  },
  "prettier": "@readme/eslint-config/prettier"
}
