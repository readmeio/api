{
  "name": "api",
  "version": "5.0.0-beta.1",
  "description": "Generate an SDK from an OpenAPI definition",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "api": "./bin/api"
  },
  "scripts": {
    "build": "tsc",
    "debug:bin": "node -r ts-node/register src/bin.ts",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/; npm run prebuild.packageConfig",
    "prebuild.packageConfig": "node -p \"'// This file is automatically updated by the build script.\\nexport const PACKAGE_NAME = \\'' + require('./package.json').name + '\\';\\nexport const PACKAGE_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/packageInfo.ts; git add src/packageInfo.ts",
    "prepack": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "nyc mocha \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/readmeio/api.git",
    "directory": "packages/api"
  },
  "homepage": "https://github.com/readmeio/api",
  "bugs": {
    "url": "https://github.com/readmeio/api/issues"
  },
  "author": "Jon Ursenbach <jon@readme.io>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@readme/oas-to-har": "^17.0.8",
    "@readme/openapi-parser": "^2.2.0",
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "datauri": "^4.1.0",
    "execa": "^5.1.1",
    "fetch-har": "^8.0.3",
    "figures": "^3.2.0",
    "find-cache-dir": "^3.3.1",
    "form-data-encoder": "^1.7.2",
    "formdata-node": "^4.3.2",
    "get-stream": "^6.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^10.1.5",
    "json-schema-traverse": "^1.0.0",
    "lodash.merge": "^4.6.2",
    "make-dir": "^3.1.0",
    "oas": "^18.3.3",
    "object-hash": "^3.0.0",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "ssri": "^9.0.0",
    "ts-morph": "^15.1.0",
    "validate-npm-package-name": "^4.0.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^8.8.1",
    "@readme/oas-examples": "^5.4.1",
    "@types/chai": "^4.3.1",
    "@types/find-cache-dir": "^3.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.merge": "^4.6.7",
    "@types/mocha": "^9.1.1",
    "@types/object-hash": "^2.2.1",
    "@types/prompts": "^2.0.14",
    "@types/sinon-chai": "^3.2.8",
    "@types/ssri": "^7.1.1",
    "@types/validate-npm-package-name": "^4.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.19.0",
    "fetch-mock": "^9.11.0",
    "mocha": "^10.0.0",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.7.4",
    "unique-temp-dir": "^1.0.0"
  },
  "prettier": "@readme/eslint-config/prettier",
  "nyc": {
    "exclude": [
      "dist/",
      "test/"
    ]
  }
}
