{
  "name": "api",
  "version": "5.0.0-beta.3",
  "description": "Magical SDK generation from an OpenAPI definition ðŸª„",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "api": "./bin/api"
  },
  "scripts": {
    "build": "tsc",
    "debug:bin": "node -r ts-node/register src/bin.ts",
    "prebuild": "rm -rf dist/; npm run prebuild.packageConfig",
    "prebuild.packageConfig": "node -p \"'// This file is automatically updated by the build script.\\nexport const PACKAGE_NAME = \\'' + require('./package.json').name + '\\';\\nexport const PACKAGE_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/packageInfo.ts; git add src/packageInfo.ts",
    "prepack": "npm run build",
    "test": "nyc mocha \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/readmeio/api.git",
    "directory": "packages/api"
  },
  "homepage": "https://api.readme.dev",
  "bugs": {
    "url": "https://github.com/readmeio/api/issues"
  },
  "author": "Jon Ursenbach <jon@readme.io>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "api",
    "openapi",
    "sdk",
    "swagger"
  ],
  "dependencies": {
    "@readme/oas-to-har": "^19.2.0",
    "@readme/openapi-parser": "^2.2.0",
    "caseless": "^0.12.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "datauri": "^4.1.0",
    "execa": "^5.1.1",
    "fetch-har": "^8.1.3",
    "figures": "^3.2.0",
    "find-cache-dir": "^3.3.1",
    "form-data-encoder": "^1.7.2",
    "formdata-node": "^4.3.2",
    "get-stream": "^6.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-ts": "^2.5.5",
    "json-schema-traverse": "^1.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.deburr": "^4.1.0",
    "lodash.merge": "^4.6.2",
    "lodash.setwith": "^4.3.2",
    "lodash.startcase": "^4.4.0",
    "make-dir": "^3.1.0",
    "node-abort-controller": "^3.0.1",
    "oas": "^19.0.1",
    "ora": "^5.4.1",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "remove-undefined-objects": "^2.0.1",
    "ssri": "^10.0.0",
    "ts-morph": "^16.0.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@readme/oas-examples": "^5.7.1",
    "@types/caseless": "^0.12.2",
    "@types/chai": "^4.3.1",
    "@types/find-cache-dir": "^3.2.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.deburr": "^4.1.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.setwith": "^4.3.7",
    "@types/lodash.startcase": "^4.4.7",
    "@types/mocha": "^10.0.0",
    "@types/prettier": "^2.7.1",
    "@types/prompts": "^2.0.14",
    "@types/sinon-chai": "^3.2.8",
    "@types/ssri": "^7.1.1",
    "@types/validate-npm-package-name": "^4.0.0",
    "chai": "^4.3.6",
    "fetch-mock": "^9.11.0",
    "mocha": "^10.0.0",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.7.4",
    "unique-temp-dir": "^1.0.0"
